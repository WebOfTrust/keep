{
  "name": "keep",
  "version": "1.0.0",
  "description": "Electron based application for managing KERI identifiers",
  "author": {
    "name": "Kevin Griffin",
    "email": "griffin.kev@gmail.com"
  },
  "homepage": "https://weboftrust.github.io/keep",
  "license": "Apache-2.0",
  "scripts": {
    "package:root-gar": "NODE_ENV=root-gar-local parcel build src/index.html --public-url ./ --cache-dir ward/.parcel-cache-dev --dist-dir app/static",
    "package:root-gar-zero": "NODE_ENV=root-gar-local-zero parcel build src/index.html --public-url ./ --cache-dir ward/.parcel-cache-dev --dist-dir app/static",
    "package:root-gar-one": "NODE_ENV=root-gar-local-one parcel build src/index.html --public-url ./ --cache-dir ward/.parcel-cache-dev --dist-dir app/static",
    "package:root-gar-two": "NODE_ENV=root-gar-local-two parcel build src/index.html --public-url ./ --cache-dir ward/.parcel-cache-dev --dist-dir app/static",
    "build:generic": "yarn rm-build; yarn set-tasks:generic; parcel build src/index.html --cache-dir ward/.parcel-cache --dist-dir ward/build-ui-root-gar --public-url ./",
    "start:generic": "parcel src/index.html --cache-dir ward/.parcel-cache-dev --dist-dir ward/build-ui-root-gar",
    "start:lead-root-gar": "NODE_ENV=lead-root-gar parcel src/index.html --port 5520 --cache-dir .webcache/.lead-root-gar-cache-dev --dist-dir .webcache/lead-root-gar-build-dev",
    "start:root-gar": "NODE_ENV=root-gar-local parcel src/index.html --port 5521 --cache-dir .webcache/.root-gar-cache-dev --dist-dir .webcache/root-gar-build-dev",
    "start:lead-external-gar": "NODE_ENV=lead-external-gar parcel src/index.html --port 5522 --cache-dir .webcache/.lead-external-gar-cache-dev --dist-dir .webcache/lead-external-gar-build-dev",
    "start:external-gar": "NODE_ENV=external-gar parcel src/index.html --port 5523 --cache-dir .webcache/.external-gar-cache-dev --dist-dir .webcache/external-gar-build-dev",
    "start:lead-internal-gar": "NODE_ENV=lead-internal-gar parcel src/index.html --port 5524 --cache-dir .webcache/.lead-internal-gar-cache-dev --dist-dir .webcache/lead-internal-gar-build-dev",
    "start:internal-gar": "NODE_ENV=internal-gar parcel src/index.html --port 5525 --cache-dir .webcache/.internal-gar-cache-dev --dist-dir .webcache/internal-gar-build-dev",
    "start:lead-qar": "NODE_ENV=lead-qar parcel src/index.html --port 5526 --cache-dir .webcache/.lead-qar-cache-dev --dist-dir .webcache/lead-qar-build-dev",
    "start:qar": "NODE_ENV=qar parcel src/index.html --port 5527 --cache-dir .webcache/.qar-cache-dev --dist-dir .webcache/qar-build-dev",
    "start:lead-lar": "NODE_ENV=lead-lar parcel src/index.html --port 5528 --cache-dir .webcache/.lead-lar-cache-dev --dist-dir .webcache/lead-lar-build-dev",
    "start:lar": "NODE_ENV=lar parcel src/index.html --port 5529 --cache-dir .webcache/.lar-cache-dev --dist-dir .webcache/lar-build-dev",
    "start:person": "NODE_ENV=person parcel src/index.html --port 5530 --cache-dir .webcache/.person-cache-dev --dist-dir .webcache/person-build-dev",
    "set-tasks:generic": "plop tasks-index 'generic' --force",
    "rm-build": "rm -rf ward/.parcel-cache ward/build-ui-root-gar ward/.parcel-cache-dev ward/build-ui-dev ward/.parcel-cache-dev-lead ward/build-ui-dev-lead",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "eslint src/",
    "format": "prettier --write src/**/*.{js,html,jsx,json,scss,css,md} --no-error-on-unmatched-pattern",
    "nuke": "rm -rf node_modules/; yarn rm-build; yarn pages:rm-build",
    "pages:start": "parcel pages/index.html",
    "pages:rm-build": "rm -rf .parcel-cache dist",
    "pages:build": "yarn pages:rm-build; parcel build pages/index.html --public-url ./",
    "prepages:deploy": "yarn pages:build",
    "pages:deploy": "gh-pages -d dist",
    "wdio": "wdio run wdio.conf.js"
  },
  "targets": {
    "main": false
  },
  "dependencies": {
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/material-icons-outlined": "^4.5.4",
    "@popperjs/core": "^2.11.5",
    "emoji-button": "^2.2.2",
    "emoji-picker-element": "^1.11.3",
    "fs-extra": "^10.1.0",
    "json": "^11.0.0",
    "material-components-web": "^13.0.0",
    "minimist": "^1.2.6",
    "mithril": "^2.0.4",
    "moment": "^2.29.3",
    "node-powershell": "^5.0.1",
    "pattern-matching-js": "^0.2.0",
    "picmo": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.18.6",
    "@parcel/transformer-sass": "^2.0.1",
    "@wdio/cli": "^7.20.5",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/mocha-framework": "^7.20.3",
    "@wdio/spec-reporter": "^7.20.3",
    "babel-jest": "^27.2.4",
    "chromedriver": "^103.0.0",
    "eslint": "^8.0.1",
    "faker": "^5.5.3",
    "gh-pages": "^3.2.3",
    "jest": "^27.0.6",
    "jest-transform-stub": "^2.0.0",
    "jslint": "^0.12.1",
    "mithril-query": "^4.0.1",
    "parcel": "^2.0.1",
    "plop": "^3.0.5",
    "prettier": "^2.3.2",
    "sass": "^1.43.2",
    "wdio-chromedriver-service": "^7.3.2"
  }
}
